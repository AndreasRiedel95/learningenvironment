<% extend('admin_base') %>
<div class="content mb-20">
	<p class="mb-20">Alle Sections in der Datenbank:</p>
	<ul>
		<% for(let i = 0; i < sections.length; i++) { %>
			<li>
				<a href="<%=sections[i].url%>"><%=sections[i].section_number%> <%=sections[i].name%></a>
			</li>
		<% } %>
	</ul>
</div>


